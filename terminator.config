[global_config]
  window_state = hidden
  enabled_plugins = LaunchpadCodeURLHandler, APTURLHandler, LaunchpadBugURLHandler
  title_hide_sizetext = True
  focus = system
  sticky = True
  borderless = True
  geometry_hinting = False
  title_transmit_bg_color = "#d30102"
  hide_from_taskbar = True
  tab_position = hidden
[keybindings]
  cycle_prev = None
  close_window = None
  group_tab = None
  move_tab_right = None
  resize_left = None
  reset_clear = None
  go_right = None
  new_tab = None
  resize_right = None
  resize_down = None
  toggle_zoom = None
  scaled_zoom = None
  zoom_in = <Primary><Shift>underscore
  group_all = None
  full_screen = None
  prev_tab = None
  split_vert = None
  ungroup_all = None
  ungroup_tab = None
  cycle_next = None
  zoom_out = None
  go_prev = None
  close_term = None
  new_terminator = None
  go_next = None
  go_left = None
  hide_window = XF86HomePage
  reset = None
  search = None
  go_up = None
  resize_up = None
  next_tab = None
  split_horiz = None
  zoom_normal = None
  go_down = None
  new_window = None
  move_tab_left = None
  toggle_scrollbar = None
[profiles]
  [[default]]
    use_system_font = False
    visible_bell = True
    background_darkness = 0.91
    background_type = transparent
    scrollbar_position = hidden
    foreground_color = "#ffffff"
    icon_bell = False
    show_titlebar = False
    font = DejaVu Sans Mono 10
    scrollback_infinite = True
  [[solarized-dark]]
    scrollbar_position = hidden
    palette = "#073642:#dc322f:#859900:#b58900:#268bd2:#d33682:#2aa198:#eee8d5:#002b36:#cb4b16:#586e75:#657b83:#839496:#6c71c4:#93a1a1:#fdf6e3"
    visible_bell = True
    background_darkness = 0.95
    background_type = transparent
    use_system_font = False
    cursor_color = "#eee8d5"
    foreground_color = "#839496"
    icon_bell = False
    show_titlebar = False
    font = DejaVu Sans Mono 10
    background_color = "#002b36"
    scrollback_infinite = True
  [[solarized-light]]
    scrollbar_position = hidden
    palette = "#073642:#dc322f:#859900:#b58900:#268bd2:#d33682:#2aa198:#eee8d5:#002b36:#cb4b16:#586e75:#657b83:#839496:#6c71c4:#93a1a1:#fdf6e3"
    visible_bell = True
    background_darkness = 0.95
    background_type = transparent
    use_system_font = False
    cursor_color = "#002b36"
    foreground_color = "#657b83"
    icon_bell = False
    show_titlebar = False
    font = DejaVu Sans Mono 10
    background_color = "#fdf6e3"
    scrollback_infinite = True
[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = solarized-light
    [[[window0]]]
      type = Window
      parent = ""
